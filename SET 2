CREATE TABLE BRANCH (
  bname VARCHAR2(20) PRIMARY KEY,
  yoj NUMBER(4),
  intake NUMBER
);

CREATE TABLE STUDENT (
  regdno NUMBER PRIMARY KEY,
  name VARCHAR2(30),
  age NUMBER(2),
  bname VARCHAR2(20),
  perc NUMBER(5,2),
  FOREIGN KEY (bname) REFERENCES BRANCH (bname)
);




SELECT * FROM STUDENT WHERE bname IN ('CSE', 'IT');

SELECT name FROM STUDENT WHERE bname = 'CSE' AND perc > 60;

SELECT name FROM STUDENT WHERE perc = (SELECT MAX(perc) FROM STUDENT);

SELECT name FROM STUDENT WHERE age BETWEEN 15 AND 30;






DECLARE
   division_result NUMBER;
   numerator NUMBER := 20;
   denominator NUMBER := 0;  -- Will trigger exception
BEGIN
   division_result := numerator / denominator;
   DBMS_OUTPUT.PUT_LINE('Result: ' || division_result);
EXCEPTION
   WHEN ZERO_DIVIDE THEN
       DBMS_OUTPUT.PUT_LINE('Error: Division by zero detected!');
   WHEN OTHERS THEN
       DBMS_OUTPUT.PUT_LINE('Unknown error: ' || SQLERRM);
END;
/




